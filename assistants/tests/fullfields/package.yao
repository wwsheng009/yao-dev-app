{
  "name": "Full Fields Test Assistant",
  "type": "assistant",
  "avatar": "/api/__yao/app/icons/app.png",
  "connector": "gpt-4o",
  "connector_options": {
    "optional": true,
    "connectors": ["gpt-4o", "gpt-4o-mini", "deepseek"],
    "filters": ["vision", "tool_calls"]
  },
  "description": "Test assistant with all available fields for unit testing",
  "options": {
    "temperature": 0.7,
    "max_tokens": 2000,
    "top_p": 0.9,
    "presence_penalty": 0.1,
    "frequency_penalty": 0.2
  },
  "kb": {
    "collections": ["test-collection"],
    "options": {
      "top_k": 5,
      "score_threshold": 0.7
    }
  },
  "db": {
    "models": ["test-model"],
    "options": {
      "limit": 100,
      "offset": 0
    }
  },
  "mcp": {
    "servers": [
      {
        "server_id": "echo",
        "tools": ["ping", "echo"]
      }
    ],
    "options": {
      "timeout": 30
    }
  },
  "workflow": {
    "workflows": ["test-workflow"],
    "options": {
      "max_steps": 10
    }
  },
  "uses": {
    "vision": "agent",
    "audio": "mcp:audio-server",
    "fetch": "agent",
    "web": "builtin",
    "keyword": "builtin",
    "querydsl": "builtin",
    "rerank": "builtin"
  },
  "search": {
    "web": {
      "provider": "tavily",
      "max_results": 15
    },
    "kb": {
      "collections": ["docs", "faq"],
      "threshold": 0.8,
      "graph": true
    },
    "db": {
      "models": ["user", "product"],
      "max_results": 50
    },
    "citation": {
      "format": "#ref:{id}",
      "auto_inject_prompt": true
    },
    "weights": {
      "user": 1.0,
      "hook": 0.9,
      "auto": 0.7
    }
  },
  "placeholder": {
    "title": "Full Fields Test",
    "description": "Test assistant with complete field coverage",
    "prompts": [
      "Test all fields",
      "Verify prompt presets",
      "Check connector options"
    ]
  },
  "tags": ["Test", "Development", "FullFields"],
  "modes": ["chat", "task", "analyze"],
  "default_mode": "chat",
  "sort": 100,
  "public": true,
  "readonly": true,
  "mentionable": true,
  "automated": false,
  "disable_global_prompts": true,
  "share": "team"
}
