{
  "name": "Search Auto Keyword Test",
  "description": "Test assistant for auto web search with keyword extraction",
  "connector": "deepseek.v3",
  "search": {
    "web": {
      "provider": "tavily",
      "api_key_env": "TAVILY_API_KEY",
      "max_results": 3
    },
    "keyword": {
      "max_keywords": 5,
      "language": "auto"
    },
    "citation": {
      "format": "xml",
      "auto_inject_prompt": true
    }
  },
  "uses": {
    "search": "builtin",
    "web": "builtin",
    "keyword": "builtin"
  }
}

