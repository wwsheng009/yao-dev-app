{
  "name": "Search Auto Web Test",
  "description": "Test assistant for auto web search integration",
  "connector": "deepseek.v3",
  "search": {
    "web": {
      "provider": "tavily",
      "api_key_env": "TAVILY_API_KEY",
      "max_results": 3
    },
    "citation": {
      "format": "xml",
      "auto_inject_prompt": true
    }
  },
  "uses": {
    "search": "builtin",
    "web": "builtin"
  }
}
