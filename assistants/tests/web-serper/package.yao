{
  "name": "Web Serper Test Assistant",
  "type": "assistant",
  "avatar": "/api/__yao/app/icons/search.png",
  "connector": "gpt-4o",
  "description": "Dedicated assistant for testing the Serper web search provider",
  "tags": ["Test", "Search", "Web", "Serper"],
  "sort": 202,
  "public": false,
  "readonly": false,
  "mentionable": true,
  "automated": true,
  "share": "private",
  "uses": {
    "web": "builtin",
    "search": "builtin"
  },
  "search": {
    "web": {
      "provider": "serper",
      "api_key_env": "$ENV.SERPER_API_KEY",
      "max_results": 10
    },
    "citation": {
      "format": "#ref:{id}",
      "auto_inject_prompt": true
    },
    "weights": {
      "user": 1.0,
      "hook": 0.8,
      "auto": 0.6
    }
  }
}
